extends layout

block title
  title #{__('page.title.signup')}

block content
  .panel(class='panel-default')
    .panel-heading
        h3.panel-title #{__('signup.block.title')}
      .panel-body
        .alert.alert-danger
          button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
          strong #{__('error')}
          span= error

        +form(form)
